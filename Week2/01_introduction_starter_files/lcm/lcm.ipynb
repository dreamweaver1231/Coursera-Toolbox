   "source": [
    "# Uses python3\n",
    "def gcd_naive(a, b):\n",
    "\n",
    "    if b == 0:\n",
    "        return a\n",
    "\n",
    "    a_prime = a % b\n",
    "    return gcd_naive(b, a_prime)\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    n = input()\n",
    "    a, b = map(int, n.split())\n",
    "    print(((a*b)//(gcd_naive(a, b))))\n"
   ]
